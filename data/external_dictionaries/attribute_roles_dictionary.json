  },
  "statistical_thresholds": {
    "entropy_high": 5.0,
    "entropy_mid": 3.0,
    "uniqueness_high": 0.9,
    "uniqueness_low": 0.2,
    "text_length_long": 100,
    "mvf_threshold": 2
  }
