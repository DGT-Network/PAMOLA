  "industry_specific": {
    "healthcare": {
      "SENSITIVE_ATTRIBUTE": {
        "en": ["icd_code", "diagnosis", "prescription", "clinical_note", "patient", "medical", "blood_type", "allergy"],
        "ru": ["мкб", "диагноз", "рецепт", "запись_врача", "пациент", "медицинский", "группа_крови", "аллергия"]
      },
      "QUASI_IDENTIFIER": {
        "en": ["hospital", "doctor_name", "clinic", "ward"],
        "ru": ["больница", "врач", "клиника", "отделение"]
      }
    },
    "finance": {
      "DIRECT_IDENTIFIER": {
        "en": ["account_id", "transaction_id", "iban", "swift", "card_number"],
        "ru": ["номер_счета", "ид_транзакции", "ибан", "свифт", "номер_карты"]
      },
      "SENSITIVE_ATTRIBUTE": {
