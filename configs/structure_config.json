{
    "INCLUDE_EXTENSIONS": [".py", ".json", ".png", ".md", ".yaml", ".yml", ".csv", ".parquet", ".sql", ".txt"],
    "EXCLUDED_DIRS": [
        ".git", "__pycache__", ".venv", ".venv1", "venv", "backups", "logs", ".obsidian",
        "site", "env", "node_modules", ".pytest_cache", ".mypy_cache", ".ruff_cache",
        "dist", "build", ".idea", ".vs", ".vscode", "__MACOSX", "tmp", "temp"
    ],
    "INCLUDE_GIT": false,
    "INCLUDE_AST": true,
    "INCLUDE_DETAILS": false,
    "EXPORT_CSV": false,
    "AI_FRIENDLY": true,
    "TREE_VIEW": true,
    "PROGRESS_DISPLAY": true,
    "PROGRESS_STYLE": "bar",
    "USE_TQDM": true,
    "PARALLEL_PROCESSING": true,
    "MAX_WORKERS": 8,
    "VERBOSE": 2,
    "AI_EXCLUDE_EMPTY_FILES": true,
    "AI_INCLUDE_FILE_STATS": true,
    "AI_SIMPLIFY_PATHS": true,
    "AI_INCLUDE_IMPORTS": true,
    "AI_INCLUDE_CLASSES": true,
    "AI_INCLUDE_FUNCTIONS": true,
    "AI_MAX_FILE_COUNT": 1000
}