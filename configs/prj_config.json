{
  "data_repository": "D:/VK/_DEVEL/PAMOLA/data",
  "directory_structure": {
    "raw": "raw",
    "processed": "processed",
    "logs": "logs",
    "configs": "configs"
  },
  "logging": {
    "level": "INFO",
    "file": "pamola_processing.log",
    "format": "%(asctime)s - %(levelname)s - %(name)s - %(message)s"
  },
  "performance": {
    "chunk_size": 100000,
    "default_encoding": "utf-8",
    "default_delimiter": ",",
    "memory_limit_mb": 1000
  },
  "tasks": {
    "identification": {
      "dependencies": [],
      "continue_on_error": false,
      "use_encryption": false
    },
    "details": {
      "dependencies": ["identification"],
      "continue_on_error": true,
      "use_encryption": false
    },
    "contacts": {
      "dependencies": ["identification"],
      "continue_on_error": true,
      "use_encryption": false
    }
  }
}