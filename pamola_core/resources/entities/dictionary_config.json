{
  "entity_extractor": {
    "name": "GenericDictionaryExtractor",
    "description": "A universal entity extractor that works with any type of entity dictionaries, allowing for custom entity types beyond the specialized extractors provided in the package.",
    "parameters": {
      "entity_type": {
        "description": "Type of entities to extract (used for dictionary lookup)",
        "default": "generic",
        "type": "string",
        "supported_types": ["generic", "person", "organization", "location", "product", "job", "skill", "education", "industry", "medical", "financial"]
      },
      "fallback_to_ner": {
        "description": "Whether to fall back to NER if dictionary matching fails",
        "default": true,
        "type": "boolean"
      },
      "dictionary_path": {
        "description": "Path to the dictionary file for entity extraction",
        "default": null,
        "type": "string"
      },
      "language": {
        "description": "Language for entity extraction",
        "default": "en",
        "type": "string",
        "supported_languages": ["en", "ru", "fr", "de", "es", "pt", "ja", "zh", "ko", "vi"]
      },
      "min_confidence": {
        "description": "Minimum confidence threshold for entity matches",
        "default": 0.4,
        "type": "float"
      },
      "match_method": {
        "description": "Method used for matching entities",
        "default": "exact",
        "type": "string",
        "supported_methods": ["exact", "fuzzy", "regex", "bert_similarity", "combined"]
      },
      "max_edit_distance": {
        "description": "Maximum Levenshtein edit distance for fuzzy matching",
        "default": 2,
        "type": "integer"
      },
      "case_sensitive": {
        "description": "Whether matching should be case-sensitive",
        "default": false,
        "type": "boolean"
      }
    },
    "methods": {
      "_get_entity_type": {
        "description": "Get the entity type for this extractor",
        "returns": "Entity type string"
      },
      "_find_dictionary": {
        "description": "Find a dictionary file for the current entity type",
        "returns": "Path to the dictionary file if found, None otherwise"
      },
      "_extract_with_ner": {
        "description": "Extract entities using NER as a fallback",
        "parameters": {
          "text": "Original text",
          "normalized_text": "Normalized text",
          "language": "Language of the text"
        },
        "returns": "EntityMatchResult if found, None otherwise"
      },
      "_extract_with_dictionary": {
        "description": "Extract entities using dictionary-based lookup",
        "parameters": {
          "text": "Original text",
          "normalized_text": "Normalized text"
        },
        "returns": "List of EntityMatchResult objects"
      },
      "_resolve_conflicts": {
        "description": "Resolve conflicts between multiple entity matches",
        "parameters": {
          "matches": "List of entity matches"
        },
        "returns": "List of filtered entity matches"
      },
      "extract": {
        "description": "Main extraction method that combines dictionary and NER approaches",
        "parameters": {
          "text": "Text to extract entities from",
          "language": "Language of the text (optional)"
        },
        "returns": "List of EntityMatchResult objects"
      }
    },
    "ner_capabilities": {
      "priority_entity_types": [
        "PERSON",
        "ORG",
        "GPE",
        "LOC",
        "PRODUCT",
        "EVENT",
        "WORK_OF_ART",
        "DATE",
        "TIME",
        "MONEY",
        "QUANTITY",
        "ORDINAL",
        "CARDINAL",
        "PERCENT",
        "LANGUAGE",
        "NORP"
      ],
      "confidence": 0.5,
      "method": "ner",
      "supported_models": [
        {
          "name": "spacy",
          "languages": ["en", "fr", "de", "es", "pt", "ru", "ja", "zh"],
          "entity_types": ["PERSON", "ORG", "GPE", "LOC", "PRODUCT"]
        },
        {
          "name": "flair",
          "languages": ["en", "de"],
          "entity_types": ["PERSON", "ORG", "LOC"]
        },
        {
          "name": "custom_bert",
          "languages": ["en", "ru", "vi"],
          "entity_types": ["PERSON", "ORG", "LOC", "JOB", "SKILL"]
        }
      ]
    },
    "inheritance": {
      "parent": "BaseEntityExtractor",
      "required_interfaces": [
        "EntityMatchResult"
      ],
      "override_methods": [
        "extract",
        "_get_entity_type",
        "_preprocess_text",
        "_postprocess_results"
      ]
    },
    "dependencies": {
      "internal": [
        "core.utils.nlp.entity.base",
        "core.utils.nlp.model_manager",
        "core.utils.nlp.preprocessing",
        "core.utils.nlp.tokenization"
      ],
      "external": [
        "spacy",
        "flair",
        "rapidfuzz",
        "regex",
        "transformers"
      ]
    },
    "dictionary_format": {
      "file_types": ["json", "csv", "txt", "sqlite"],
      "required_fields": ["term", "type"],
      "optional_fields": ["aliases", "confidence", "domain", "level", "notes"],
      "example": {
        "term": "Microsoft",
        "type": "ORG",
        "aliases": ["Microsoft Corporation", "MSFT"],
        "confidence": 0.95,
        "domain": "Technology",
        "level": 1
      }
    }
  },
  "usage_examples": [
    {
      "description": "Extract generic entities from text",
      "code": "extractor = GenericDictionaryExtractor(entity_type='generic', language='en')\nresult = extractor.extract('John Smith works at Microsoft')",
      "expected_result": {
        "entities": [
          {
            "text": "John Smith",
            "type": "PERSON",
            "confidence": 0.85
          },
          {
            "text": "Microsoft",
            "type": "ORG",
            "confidence": 0.92
          }
        ]
      }
    },
    {
      "description": "Extract custom entity type without NER fallback",
      "code": "extractor = GenericDictionaryExtractor(entity_type='company', fallback_to_ner=False, language='en')\nresult = extractor.extract('The company was founded in 2005')"
    },
    {
      "description": "Extract job titles with fuzzy matching",
      "code": "extractor = GenericDictionaryExtractor(entity_type='job', match_method='fuzzy', max_edit_distance=3, language='en')\nresult = extractor.extract('She is the Senir Softare Enginear at Google')",
      "expected_result": {
        "entities": [
          {
            "text": "Senir Softare Enginear",
            "normalized": "Senior Software Engineer",
            "type": "JOB",
            "confidence": 0.78
          },
          {
            "text": "Google",
            "type": "ORG",
            "confidence": 0.95
          }
        ]
      }
    },
    {
      "description": "Extract medical terms in Russian text",
      "code": "extractor = GenericDictionaryExtractor(entity_type='medical', language='ru')\nresult = extractor.extract('Пациент был госпитализирован с диагнозом пневмония')",
      "expected_result": {
        "entities": [
          {
            "text": "госпитализирован",
            "type": "MEDICAL_PROCEDURE",
            "confidence": 0.72
          },
          {
            "text": "пневмония",
            "type": "MEDICAL_CONDITION",
            "confidence": 0.88
          }
        ]
      }
    },
    {
      "description": "Extract locations with high confidence threshold",
      "code": "extractor = GenericDictionaryExtractor(entity_type='location', min_confidence=0.8, language='en')\nresult = extractor.extract('We traveled from Paris to Berlin last summer')",
      "expected_result": {
        "entities": [
          {
            "text": "Paris",
            "type": "CITY",
            "confidence": 0.94
          },
          {
            "text": "Berlin",
            "type": "CITY",
            "confidence": 0.93
          }
        ]
      }
    }
  ],
  "performance_benchmarks": {
    "accuracy": {
      "person_extraction": 0.92,
      "organization_extraction": 0.88,
      "location_extraction": 0.94,
      "job_title_extraction": 0.85
    },
    "speed": {
      "dictionary_lookup": "5ms per entity",
      "ner_fallback": "20ms per entity"
    },
    "memory_usage": {
      "small_dictionary": "10-20MB",
      "large_dictionary": "50-100MB"
    }
  },
  "localization": {
    "supported_languages": [
      {
        "code": "en",
        "name": "English",
        "dictionaries": ["generic", "person", "organization", "location", "job", "skill"]
      },
      {
        "code": "ru",
        "name": "Russian",
        "dictionaries": ["generic", "person", "organization", "location", "job"]
      },
      {
        "code": "fr",
        "name": "French",
        "dictionaries": ["generic", "person", "organization", "location"]
      },
      {
        "code": "de",
        "name": "German",
        "dictionaries": ["generic", "person", "organization", "location"]
      },
      {
        "code": "vi",
        "name": "Vietnamese",
        "dictionaries": ["generic", "person", "location"]
      }
    ]
  }
}
