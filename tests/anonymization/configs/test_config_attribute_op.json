{
  "basic_attribute_suppression": {
    "description": "Basic attribute suppression configuration",
    "config": {
      "field_name": "sensitive_field",
      "mode": "REMOVE",
      "save_suppressed_schema": true
    }
  },
  "multiple_fields_suppression": {
    "description": "Multiple fields suppression configuration",
    "config": {
      "field_name": "primary_field", 
      "additional_fields": ["secondary_field", "tertiary_field"],
      "mode": "REMOVE",
      "save_suppressed_schema": true
    }
  },
  "conditional_suppression": {
    "description": "Conditional suppression based on field values",
    "config": {
      "field_name": "sensitive_field",
      "mode": "REMOVE",
      "condition_field": "category",
      "condition_values": ["high_risk", "confidential"],
      "condition_operator": "in",
      "save_suppressed_schema": true
    }
  },
  "risk_based_suppression": {
    "description": "Risk-based suppression using k-anonymity threshold",
    "config": {
      "field_name": "k_anonymity_score",
      "mode": "REMOVE",
      "ka_risk_field": "k_anonymity_score",
      "risk_threshold": 3.0,
      "save_suppressed_schema": true
    }
  },
  "complex_conditional_suppression": {
    "description": "Complex conditional suppression with multiple conditions",
    "config": {
      "field_name": "sensitive_field",
      "mode": "REMOVE",
      "condition_field": "status",
      "condition_values": ["active", "inactive"],
      "condition_operator": "in",
      "save_suppressed_schema": true
    }
  },
  "performance_optimized": {
    "description": "Performance optimized suppression with memory optimization",
    "config": {
      "field_name": "large_field",
      "mode": "REMOVE",
      "optimize_memory": true,
      "adaptive_chunk_size": true,
      "chunk_size": 5000,
      "save_suppressed_schema": false
    }
  },
  "dask_distributed": {
    "description": "Distributed processing with Dask",
    "config": {
      "field_name": "distributed_field",
      "mode": "REMOVE",
      "use_dask": true,
      "npartitions": 4,
      "dask_partition_size": "100MB",
      "save_suppressed_schema": true
    }
  },
  "parallel_processing": {
    "description": "Parallel processing with joblib",
    "config": {
      "field_name": "parallel_field",
      "mode": "REMOVE",
      "use_vectorization": true,
      "parallel_processes": 2,
      "chunk_size": 1000,
      "save_suppressed_schema": true
    }
  },
  "with_visualization": {
    "description": "Suppression with visualization generation",
    "config": {
      "field_name": "visual_field",
      "mode": "REMOVE",
      "generate_visualization": true,
      "visualization_backend": "plotly",
      "visualization_theme": "light",
      "save_suppressed_schema": true
    }
  },
  "with_encryption": {
    "description": "Suppression with encrypted output",
    "config": {
      "field_name": "encrypted_field",
      "mode": "REMOVE",
      "use_encryption": true,
      "encryption_mode": "AES",
      "save_output": true,
      "output_format": "csv",
      "save_suppressed_schema": true
    }
  },
  "cache_enabled": {
    "description": "Suppression with caching enabled",
    "config": {
      "field_name": "cached_field",
      "mode": "REMOVE",
      "use_cache": true,
      "force_recalculation": false,
      "save_suppressed_schema": true
    }
  },
  "invalid_mode": {
    "description": "Invalid mode configuration for error testing",
    "config": {
      "field_name": "test_field",
      "mode": "INVALID_MODE",
      "save_suppressed_schema": true
    }
  },
  "range_condition": {
    "description": "Range-based conditional suppression",
    "config": {
      "field_name": "numeric_field",
      "mode": "REMOVE",
      "condition_field": "score",
      "condition_values": [0.5, 0.8],
      "condition_operator": "range",
      "save_suppressed_schema": true
    }
  },
  "minimal_config": {
    "description": "Minimal configuration with just required parameters",
    "config": {
      "field_name": "minimal_field",
      "mode": "REMOVE"
    }
  }
}
